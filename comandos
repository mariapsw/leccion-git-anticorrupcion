#Computadora de Jorge
git config user.name jorgepsw
git config user.email mguarand@espol.edu.ec
git clone git clone https://github.com/mariapsw/leccion-git-anticorrupcion
cd leccion-git-anticorrupcion
git branch desarrollo
git branch produccion
touch index.htm
git add index.htm
git commit -m "creo index.htm"
git checkout desarrollo
touch acerca.htm
nano acerca.htm
git commit -m "creado acerca.htm"
git checkout produccion
git merge master
git push origin produccion

